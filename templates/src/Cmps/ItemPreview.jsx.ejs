export function <%= entityName.charAt(0).toUpperCase() + entityName.slice(1) %>Preview({ <%= entityName %> }) {

    return (
        <section className="<%= entityName %>-preview flex">
            <h2>{<%= entityName %>.name}</h2>
            <img src={<%= entityName %>.imgUrl} alt="<%= entityName %>.name"></img>
        </section>
    );
}
